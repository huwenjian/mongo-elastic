# 查看linux yum镜像源
    vim /etc/yum.repos.d/CentOS7-Base-163.repo

# 163的yum源配置命令配置命令
    cd /etc/yum.repos.d/
    rename .repo .repo.bak *.repo
    wget http://mirrors.163.com/.help/CentOS7-Base-163.repo
    yum clean all
    yum makecache



















# 导出文档
    yum -y install lrzsz
    # 导出1.log
        sz 1.log
